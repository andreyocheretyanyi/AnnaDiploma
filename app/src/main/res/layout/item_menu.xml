  android:visibility="gone"
  android:id="@+id/main_menu_root"
  xmlns:android="http://schemas.android.com/apk/res/android"
  xmlns:app="http://schemas.android.com/apk/res-auto"
  xmlns:tools="http://schemas.android.com/tools"
  android:orientation="vertical"
  android:layout_width="match_parent"
  android:layout_height="match_parent">

  <android.support.constraint.ConstraintLayout
    android:clickable="false"
    android:visibility="invisible"
    android:id="@+id/item_menu_1"
    android:background="@color/colorSecondBLight"
    android:layout_weight="1"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

  <View
    android:layout_width="0.1dp"
    android:layout_height="0.1dp"
    android:id="@+id/view1"
    app:layout_constraintTop_toTopOf="parent"
    app:layout_constraintBottom_toBottomOf="parent"
    app:layout_constraintRight_toRightOf="parent"
    app:layout_constraintLeft_toLeftOf="parent"
    tools:layout_constraintRight_creator="1"
    tools:layout_constraintLeft_creator="1"
    app:layout_constraintHorizontal_bias="0.501"/>
    <TextView
      android:id="@+id/item_menu_text_1"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:text="Map"
      tools:layout_constraintBottom_creator="1"
      app:layout_constraintLeft_toRightOf="@+id/view1"
      android:layout_marginLeft="4dp"
      app:layout_constraintTop_toTopOf="@+id/view1"
      android:layout_marginTop="8dp"
      app:layout_constraintBottom_toBottomOf="@+id/view1"
      android:layout_marginBottom="0dp"
      app:layout_constraintVertical_bias="0.791"/>
    <ImageView
      android:id="@+id/imageView_item_menu_1"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      app:srcCompat="@mipmap/ic_launcher"
      app:layout_constraintRight_toLeftOf="@+id/view1"
      android:layout_marginRight="0dp"
      app:layout_constraintTop_toTopOf="@+id/view1"
      android:layout_marginTop="0dp"
      app:layout_constraintBottom_toBottomOf="@+id/view1"
      android:layout_marginBottom="0dp"
      app:layout_constraintVertical_bias="0.509"/>
  </android.support.constraint.ConstraintLayout>

